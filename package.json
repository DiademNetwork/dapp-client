{
  "name": "diadem-dapp-boilerplate",
  "version": "2.0.0",
  "description": "React boilerplate for diadem dapps",
  "main": "index.js",
  "scripts": {
    "clean-dist": "./scripts/clean-dist.js",
    "copy-public": "./scripts/copy-public.js",
    "cypress:open": "npx cypress open",
    "lint": "eslint src/ scripts/ config/ --ignore-path src/configurables/blockchains/bip39.english.js",
    "prebuild": "yarn run clean-dist && yarn run copy-public",
    "build": "./scripts/build.js",
    "build-storybook": "build-storybook",
    "predeploy": "yarn build",
    "deploy": "aws s3 sync dist/ s3://diadem.network --delete",
    "start": "yarn run clean-dist && yarn run copy-public && webpack-dev-server --config ./webpackConfigs/webpack.development.js --https",
    "start:sandbox": "yarn run clean-dist && yarn run copy-public && webpack-dev-server --config ./webpackConfigs/webpack.sandbox.js",
    "start:sandbox:server": "nodemon ./sandboxApi/index.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --coverage",
    "test:cypress": "npx cypress run --record=false"
  },
  "author": "Clement Bresson",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "axios-mock-adapter": "^1.15.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.23.0",
    "chalk": "^2.3.1",
    "css-loader": "^0.28.9",
    "cypress": "^3.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.4",
    "fs-extra": "^5.0.0",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.2",
    "nodemon": "^1.18.9",
    "prop-types": "^15.6.0",
    "react": "16.7.0-alpha.2",
    "react-dom": "16.7.0-alpha.2",
    "react-redux": "6.0.0",
    "redux": "^3.7.2",
    "style-loader": "^0.20.2",
    "styled-components": "^3.1.6",
    "webpack": "4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/cypress/",
      "<rootDir>/.storybook/"
    ],
    "setupFiles": [
      "./setupTests.js"
    ],
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@material-ui/core": "3.9.2",
    "@material-ui/icons": "3.0.2",
    "@material-ui/styles": "^3.0.0-alpha.10",
    "@storybook/addon-console": "^1.0.8",
    "axios": "^0.21.1",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "bignumber.js": "^7.2.1",
    "bitcoinjs-lib": "^3.3.2",
    "bs58": "^4.0.1",
    "connected-react-router": "^6.2.0-beta.3",
    "copy-to-clipboard": "^3.0.8",
    "dotenv-webpack": "^1.5.7",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-cypress": "^2.0.1",
    "faker": "^4.1.0",
    "fb": "^2.0.0",
    "file-loader": "^2.0.0",
    "getstream": "4.1.0",
    "history": "^4.7.2",
    "husky": "^1.1.4",
    "image-webpack-loader": "^4.3.1",
    "is-url": "^1.2.4",
    "loom-js": "1.38.0",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.21",
    "qtumcore-lib": "^0.0.1",
    "qtumjs": "^1.9.1",
    "qtumjs-wallet": "^0.1.5",
    "ramda": "^0.25.0",
    "react-facebook-login": "^4.0.1",
    "react-infinite-scroller": "^1.2.1",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "2.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "^3.0.1",
    "storybook-addon-material-ui": "^0.9.0-alpha.16",
    "storybook-addon-specifications": "^2.1.2",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.1",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.34"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn run lint && yarn test"
    }
  }
}
