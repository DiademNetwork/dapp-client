{
  "name": "modern-react-boilerplate",
  "version": "1.0.0",
  "description": "Modern boilerplate for React project",
  "main": "index.js",
  "scripts": {
    "build": "yarn run clean-dist && yarn run copy-public && ./scripts/build.js",
    "build-production": "yarn run lint && yarn run test && yarn run clean-dist && yarn run copy-public && ./scripts/build.js production",
    "build-storybook": "build-storybook",
    "clean-dist": "./scripts/clean-dist.js",
    "copy-public": "./scripts/copy-public.js",
    "lint": "eslint src/ scripts/ config/",
    "predeploy": "yarn build-production",
    "deploy": "gh-pages -d dist",
    "start": "yarn run clean-dist && yarn run copy-public && webpack-dev-server --config ./config/webpack.development.js",
    "storybook": "start-storybook -p 6006",
    "test": "jest --coverage"
  },
  "author": "Clement Bresson",
  "homepage": "http://clement-bresson.github.io/react-modern-boilerplate",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^3.3.14",
    "@storybook/addon-links": "^3.3.14",
    "@storybook/addons": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "6.23.0",
    "chalk": "^2.3.1",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-config-standard-jsx": "^5.0.0",
    "eslint-config-standard-react": "^6.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "fs-extra": "^5.0.0",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "redux": "^3.7.2",
    "rimraf": "^2.6.2",
    "style-loader": "^0.20.2",
    "styled-components": "^3.1.6",
    "uglifyjs-webpack-plugin": "^1.2.0",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./setupTests.js"
    ]
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "getstream": "^3.20.0",
    "qtumjs": "^1.9.1",
    "qtumjs-wallet": "^0.1.5",
    "ramda": "^0.25.0",
    "react-facebook-login": "^4.0.1",
    "semantic-ui-css": "^2.3.3",
    "semantic-ui-react": "^0.82.3",
    "url-loader": "^1.1.1"
  }
}
